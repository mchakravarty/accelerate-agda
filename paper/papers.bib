@InProceedings{Welinder1995,
  AUTHOR       = {Morten Welinder},
  YEAR         = {1995},
  TITLE        = {Very Efficient Conversions},
  BOOKTITLE    = {The 8th International Workshop on Higher Order Logic
                  Theorem Proving and Its Applications, Aspen Grove, Utah},
