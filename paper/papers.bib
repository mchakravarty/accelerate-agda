@InProceedings{AbramovGlueck1998,
  AUTHOR  =     {Abramov, Sergei M. and Gl{\"u}ck, Robert},
  YEAR    =     1998,
  TITLE   =     {Semantics Modifiers: an Approach to Non-Standard Semantics
                 of Programming Languages},
  crossref = {FLOPS1998},
  pages     =   {247--270},
  keywords  =   {programming languages, non-standard semantics,
                 inverse computation, logic programming, program
                 specialization, partial evaluation, metacomputation},
  summary   =   {An approach for systematically modifying the semantics of
                 programming languages by semantics modifiers is described. It allows the
                 design of general and reusable ``semantics components'' without the
                 penalty of being too inefficient. Inverse computation and neighborhood
                 analysis are used to demonstrate the computational feasibility of the
                 approach. Equivalence transformers are shown to fulfill the requirements
                 of semantics modifiers. Finally, projections are given that allow the
                 efficient implementation of non-standard interpreters and non-standard
                 compilers using program specialization.},
}

